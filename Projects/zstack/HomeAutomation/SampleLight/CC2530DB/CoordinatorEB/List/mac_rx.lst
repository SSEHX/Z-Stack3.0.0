    142          /*
    143           *  Macro for encoding frame control information into internal flags format.
    144           *  Parameter is pointer to the frame.  NOTE!  If either the internal frame
    145           *  format *or* the specification changes, this macro will need to be modified.
    146           */
    147          #define INTERNAL_FCF_FLAGS(p)  ((((p)[1] >> 4) & 0x03) | ((p)[0] & 0x78))
    148          
    149          /*
    150           *  The radio replaces the actual FCS with different information.  This proprietary FCS is
    151           *  the same length as the original and includes:
    152           *    1) the RSSI value
    153           *    2) the average correlation value (used for LQI)
    154           *    3) a CRC passed bit
    155           *
    156           *  These macros decode the proprietary FCS.  The macro parameter is a pointer to the two byte FCS.
    157           */
    158          #define PROPRIETARY_FCS_RSSI(p)                 ((int8)((p)[0]))
    159          #define PROPRIETARY_FCS_CRC_OK(p)               ((p)[1] & PROPRIETARY_FCS_CRC_OK_BIT)
    160          #define PROPRIETARY_FCS_CORRELATION_VALUE(p)    ((p)[1] & ~PROPRIETARY_FCS_CRC_OK_BIT)
    161          
    162          /*
    163           *  Macros for security control field.
    164           */
    165          #define SECURITY_LEVEL(s)                       (s & 0x07)
    166          #define KEY_IDENTIFIER_MODE(s)                  ((s & 0x18) >> 3)
    167          #define SECURITY_CONTROL_RESERVED(s)            ((s & 0xE0) >> 5)
