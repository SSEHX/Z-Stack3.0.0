   3219           */
   3220          ZStatus_t zclGeneral_AddAlarm( uint8 endpoint, zclGeneral_Alarm_t *alarm )
   3221          {
   3222            zclGenAlarmItem_t *pNewItem;
   3223            zclGenAlarmItem_t *pLoop;
   3224          
   3225            // Fill in the new profile list
