   2481          void ZDO_MatchEndDeviceBind( ZDEndDeviceBind_t *bindReq )
   2482          {
   2483            zAddrType_t dstAddr;
   2484            uint8 sendRsp = FALSE;
   2485            uint8 status;
   2486          
   2487            // Is this the first request?
   2488            if ( matchED == NULL )
   2489            {
   2490              // Create match info structure
   2491              matchED = (ZDMatchEndDeviceBind_t *)osal_mem_alloc( sizeof ( ZDMatchEndDeviceBind_t ) );
