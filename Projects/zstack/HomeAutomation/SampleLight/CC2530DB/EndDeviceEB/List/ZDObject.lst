   2493              {
   2494                // Clear the structure
   2495                osal_memset( (uint8 *)matchED, 0, sizeof ( ZDMatchEndDeviceBind_t ) );
   2496          
   2497                // Copy the first request's information
   2498                if ( !ZDO_CopyMatchInfo( &(matchED->ed1), bindReq ) )
   2499                {
   2500                  status = ZDP_NO_ENTRY;
   2501                  sendRsp = TRUE;
   2502                }
   2503              }
   2504              else
   2505              {
   2506                status = ZDP_NO_ENTRY;
   2507                sendRsp = TRUE;
   2508              }
   2509          
   2510              if ( !sendRsp )
   2511              {
   2512                // Set into the correct state
   2513                matchED->state = ZDMATCH_WAIT_REQ;
   2514          
   2515                // Setup the timeout
   2516                APS_SetEndDeviceBindTimeout( AIB_MaxBindingTime, ZDO_EndDeviceBindMatchTimeoutCB );
   2517              }
   2518            }
   2519            else
   2520            {
   2521                matchED->state = ZDMATCH_SENDING_BINDS;
   2522          
   2523                // Copy the 2nd request's information
   2524                if ( !ZDO_CopyMatchInfo( &(matchED->ed2), bindReq ) )
   2525                {
   2526                  status = ZDP_NO_ENTRY;
   2527                  sendRsp = TRUE;
   2528                }
   2529          
   2530                // Make a source match for ed1
   2531                matchED->ed1numMatched = ZDO_CompareClusterLists(
   2532                            matchED->ed1.numOutClusters, matchED->ed1.outClusters,
   2533                            matchED->ed2.numInClusters, matchED->ed2.inClusters, ZDOBuildBuf );
   2534                if ( matchED->ed1numMatched )
   2535                {
   2536                  // Save the match list
   2537                  matchED->ed1Matched = osal_mem_alloc( (short)(matchED->ed1numMatched * sizeof ( uint16 )) );
   2538                  if ( matchED->ed1Matched )
   2539                  {
   2540                    osal_memcpy( matchED->ed1Matched, ZDOBuildBuf, (matchED->ed1numMatched * sizeof ( uint16 )) );
   2541                  }
   2542                  else
   2543                  {
   2544                    // Allocation error, stop
   2545                    status = ZDP_NO_ENTRY;
   2546                    sendRsp = TRUE;
   2547                  }
   2548                }
   2549          
   2550                // Make a source match for ed2
   2551                matchED->ed2numMatched = ZDO_CompareClusterLists(
   2552                            matchED->ed2.numOutClusters, matchED->ed2.outClusters,
   2553                            matchED->ed1.numInClusters, matchED->ed1.inClusters, ZDOBuildBuf );
   2554                if ( matchED->ed2numMatched )
   2555                {
   2556                  // Save the match list
   2557                  matchED->ed2Matched = osal_mem_alloc( (short)(matchED->ed2numMatched * sizeof ( uint16 )) );
   2558                  if ( matchED->ed2Matched )
   2559                  {
   2560                    osal_memcpy( matchED->ed2Matched, ZDOBuildBuf, (matchED->ed2numMatched * sizeof ( uint16 )) );
   2561                  }
   2562                  else
   2563                  {
   2564                    // Allocation error, stop
   2565                    status = ZDP_NO_ENTRY;
   2566                    sendRsp = TRUE;
   2567                  }
   2568                }
