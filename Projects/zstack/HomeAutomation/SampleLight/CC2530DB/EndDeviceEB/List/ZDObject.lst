   2714          /*********************************************************************
   2715           * @fn      ZDMatchSendState()
   2716           *
   2717           * @brief   State machine for the End device match message algorithm.
   2718           *
   2719           * @param  reason - state of algoritm
   2720           * @param  status - initial message status
   2721           * @param  TransSeq - next transaction sequence number
   2722           *
   2723           * @return  FALSE if error and we are not currently matching, TRUE
   2724           *          if success.
   2725           */
   2726          uint8 ZDMatchSendState( uint8 reason, uint8 status, uint8 TransSeq )
   2727          {
   2728            uint8 *dstIEEEAddr = NULL;
   2729            uint8 dstEP = 0xFF;
   2730            zAddrType_t dstAddr;
   2731            zAddrType_t destinationAddr;
   2732            uint16 msgType;
   2733            uint16 clusterID = 0xFFFF;
   2734            ZDEndDeviceBind_t *ed = NULL;
   2735            uint8 rspStatus = ZDP_SUCCESS;
   2736          
   2737            if ( matchED == NULL )
   2738            {
   2739              return ( FALSE );
   2740            }
   2741          
   2742            // Check sequence number
   2743            if ( reason == ZDMATCH_REASON_BIND_RSP || reason == ZDMATCH_REASON_UNBIND_RSP )
   2744            {
   2745              if ( TransSeq != matchED->transSeq )
   2746              {
   2747                return( FALSE ); // ignore the message
   2748              }
   2749            }
   2750          
   2751            // turn off timer
   2752            APS_SetEndDeviceBindTimeout( 0, ZDO_EndDeviceBindMatchTimeoutCB );
   2753          
   2754            if ( reason == ZDMATCH_REASON_TIMEOUT )
   2755            {
   2756              rspStatus = ZDP_TIMEOUT;    // The process will stop
   2757            }
   2758          
   2759            if ( reason == ZDMATCH_REASON_START || reason == ZDMATCH_REASON_BIND_RSP )
   2760            {
   2761              matchED->sending = ZDMATCH_SENDING_UNBIND;
   2762          
   2763              if ( reason == ZDMATCH_REASON_BIND_RSP && status != ZDP_SUCCESS )
   2764              {
   2765                rspStatus = status;
   2766              }
   2767            }
   2768            else if ( reason == ZDMATCH_REASON_UNBIND_RSP )
   2769            {
   2770              if ( status == ZDP_SUCCESS )
   2771              {
   2772                matchED->sending = ZDMATCH_SENDING_UNBIND;
   2773              }
   2774              else
   2775              {
   2776                matchED->sending = ZDMATCH_SENDING_BIND;
   2777              }
   2778            }
   2779          
   2780            if ( reason != ZDMATCH_REASON_START && matchED->sending == ZDMATCH_SENDING_UNBIND )
   2781            {
   2782              // Move to the next cluster ID
   2783              if ( matchED->ed1numMatched )
   2784              {
   2785                matchED->ed1numMatched--;
   2786              }
   2787              else if ( matchED->ed2numMatched )
   2788              {
   2789                matchED->ed2numMatched--;
   2790              }
   2791            }
   2792          
   2793            // What message do we send now
   2794            if ( matchED->ed1numMatched )
   2795            {
   2796              ed = &(matchED->ed1);
   2797              clusterID = matchED->ed1Matched[matchED->ed1numMatched-1];
   2798              dstIEEEAddr = matchED->ed2.ieeeAddr;
   2799              dstEP = matchED->ed2.endpoint;
   2800            }
   2801            else if ( matchED->ed2numMatched )
   2802            {
   2803              ed = &(matchED->ed2);
   2804              clusterID = matchED->ed2Matched[matchED->ed2numMatched-1];
   2805              dstIEEEAddr = matchED->ed1.ieeeAddr;
   2806              dstEP = matchED->ed1.endpoint;
   2807            }
   2808          
   2809            dstAddr.addrMode = Addr16Bit;
   2810          
   2811            // Send the next message
   2812            if ( (rspStatus == ZDP_SUCCESS) && ed )
   2813            {
   2814              // Send unbind/bind message to source
   2815              if ( matchED->sending == ZDMATCH_SENDING_UNBIND )
   2816              {
   2817                msgType = Unbind_req;
   2818              }
   2819              else
   2820              {
   2821                msgType = Bind_req;
   2822              }
   2823          
   2824              dstAddr.addr.shortAddr = ed->srcAddr;
   2825          
   2826              // Save off the transaction sequence number
