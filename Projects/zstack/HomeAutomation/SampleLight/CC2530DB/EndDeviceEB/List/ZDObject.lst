   2674                osal_memcpy( (uint8*)(destReq->inClusters), (uint8 *)(srcReq->inClusters),
   2675                                (srcReq->numInClusters * sizeof ( uint16 )) );
   2676              }
   2677              else
   2678              {
   2679                allOK = FALSE;
